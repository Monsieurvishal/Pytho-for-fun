from turtle import *

for i in range(1):
    right(60)
    forward(10)
    left(120)
    forward(10)
    penup()
    right(60)
    forward(5)
    pendown()
    forward(10)
    right(180)
    forward(5)
    left(90)
    forward(10)
    right(90)
    forward(5)
    right(180)
    forward(10)
    penup()
    left(90)
    forward(10)
    right(90)
    penup()
    forward(5)
    pendown()
    forward(10)
    left(180)
    forward(10)
    left(90)
    forward(5)
    left(90)
    forward(10) 
    right(90)
    forward(5)
    right(90)
    forward(10)
    right(180)
    forward(10)
    penup()
    left(90)
    forward(10)
    right(90)
    forward(5)
    pendown()
    right(90)
    forward(10)
    left(180)
    forward(5)
    right(90)
    forward(5)
    right(90)
    forward(5)
    right(180)
    forward(10)
    right(90)
    penup()
    forward(5)
    pendown()
    right(90)
    forward(10)
    right(180)
    forward(5)
    right(90)
    forward(5)
    right(90)
    forward(5)
    right(180)
    forward(10)
    left(90)
    forward(5)
    right(180)
    forward(5)
    penup()
    forward(5)
    pendown()
    right(90)
    forward(10)
    left(90)
    forward(5)
    penup()
    forward(15)
    
done()
